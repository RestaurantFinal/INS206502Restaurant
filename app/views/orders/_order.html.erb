<div id="<%= dom_id order %>">
  <p>
    <strong>Customer name:</strong>
    <%= order.customer_name_id %>
  </p>

  <p>
    <strong>Customer email:</strong>
    <%= order.customer_email_id %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= order.address %>
  </p>

  <p>
    <strong>Total:</strong>
    <%= order.total %>
  </p>

  <p>
    <strong>Paid:</strong>
    <%= order.paid %>
  </p>

  <p>
    <strong>Food name:</strong>
    <%= order.food_name_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order.quantity %>
  </p>

</div>
